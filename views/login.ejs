<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="/stylesheets/login.css">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <title><%= title %></title>

</head>
<body>
  <div id="top_Div">
      <div id="vertical_Helper">
        <img src="/images/logo4.png" id="logo_Image">
        </div>
      </div>


      <div id="bottom_Div">
            <div id="login_Div" class="col-sm-6">
                    <table>
                        <tr>
                            <td>
                                <input class='login_input' type="text" placeholder="아이디" />
                                </br>
                            </td>
                            <td rowspan="2">
                                <button>로그인</button>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <input class='login_input' type="password" placeholder="비밀번호" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <p id="join_Button">회원가입</p>
                            </td>
                        </tr>
                    </table>
                </div>
      </div>
</body>
<script>
    $('#join_Button').click(function(){
      $(window).attr('location','/join');
    });
</script>
</html>
